{
  "name": "worldpoo.ps",
  "version": "0.0.1",
  "description": "A platform for sharing bathroom details",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "dev:backend": "nodemon --ignore './dist/' --ignore './frontend/' index.js",
    "dev:frontend": "parcel frontend/index.html",
    "build:frontend": "parcel build frontend/index.html",
    "rollback": "sequelize db:drop --url $DATABASE_URL",
    "migrate": "sequelize db:migrate --url $DATABASE_URL",
    "lint": "eslint .",
    "heroku-postbuild": "apidoc -i routes/ -o pub/api/v1/docs && npm run migrate && echo \"Migration done.\"",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DomThePorcupine/worldpoo.ps.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DomThePorcupine/worldpoo.ps/issues"
  },
  "homepage": "https://github.com/DomThePorcupine/worldpoo.ps#readme",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "bcrypt": "^3.0.7",
    "cookie-parser": "^1.4.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-cookie": "^1.0.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-react-router": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-swipeable-views": "^0.13.7",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "apidoc": "^0.20.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^20.3.1",
    "human-readable-ids": "^1.0.4",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "parcel-bundler": "^1.12.4",
    "react-router-dom": "^5.1.2",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.7.5"
  }
